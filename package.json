{
  "name": "tensorblock-studio",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "app/out-tsc/main.js",
  "scripts": {
    "dev": "concurrently \"pnpm electron:serve\" \"pnpm vite:dev\"",
    "vite:dev": "vite",
    "build": "vite build && pnpm run electron:serve-tsc",
    "build:prod": "vite build --mode production && pnpm run electron:serve-tsc",
    "electron:serve-tsc": "tsc -p app/tsconfig.json",
    "electron:serve": "wait-on tcp:5173 && pnpm run electron:serve-tsc && electron . --serve",
    "electron:build:current_platform": "rimraf ./release/ && pnpm run build:prod && electron-builder build --publish=never",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "katex": "^0.16.21",
    "lucide-react": "^0.344.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.3",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "electron": "34.2.0",
    "electron-builder": "25.1.8",
    "electron-debug": "4.1.0",
    "electron-reloader": "1.2.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rimraf": "6.0.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "wait-on": "7.2.0"
  }
}
